{% extends 'partials/base.html.twig' %}
{% set collection = page.collection() %}
{% block content %}
  <h1>Sessions</h1>
  <ul>
    {% for child in collection %}
      <li><h2>{{ child.title }}</h2></li>
      <ul>
        <li><strong>Sumary</strong>: <p>{{ child.summary }}</p></li>
        <li><strong>Format</strong>: {{ child.header.taxonomy.format }}</li>
        <li><strong>Track</strong>: {{ child.header.taxonomy.track }}</li>
      </ul>
    {% endfor %}
  </ul>
{% endblock %}